{
  "name": "rd-web",
  "version": "0.0.1",
  "description": "Rose Digital Website",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development DASHBOARD_PORT=9901 webpack-dashboard -p 9901 -c red -t dashboard -- node bin/commands.js dev",
    "build": "cross-env NODE_ENV=production node bin/commands.js build",
    "now-build": "cross-env NODE_ENV=production node bin/commands.js build && mv docroot dist",
    "build:stage": "cross-env NODE_ENV=stage node bin/commands.js build",
    "deploy:dev": "./node_modules/.bin/now -t ${NOW_TOKEN}",
    "deploy:alias-now-dev": "./node_modules/.bin/now -t ${NOW_TOKEN} alias rd-dev",
    "deploy:alias-now-qa": "./node_modules/.bin/now -t ${NOW_TOKEN} alias rd-qa",
    "deploy:alias-now-stage": "./node_modules/.bin/now -t ${NOW_TOKEN} alias rd-stage",
    "deploy:alias-now-prod": "./node_modules/.bin/now -t ${NOW_TOKEN} alias rd-prod",
    "clean": "rimraf docroot",
    "test": "jest --no-cache",
    "lint": "node bin/commands.js lint",
    "lint:fix": "node bin/commands.js lint:fix",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Rose Digital",
  "engines": {
    "npm": ">=3.10.7",
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "acorn": "^6.1.1",
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "colors": "^1.1.2",
    "concurrently": "^4.0.1",
    "config": "^2.0.1",
    "cross-env": "^5.1.3",
    "css-loader": "^2.0.0",
    "debug": "^4.1.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.3.1",
    "jest-cli": "^24.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.2.0",
    "precss": "^3.1.2",
    "prettier": "^1.16.3",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.3",
    "react-masonry-css": "^1.0.12",
    "react-test-renderer": "^16.2.0",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-s3-plugin": "^1.0.0-rc.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "base64-inline-loader": "^1.1.1",
    "braces": "^2.3.2",
    "classnames": "^2.2.6",
    "hamburgers": "^1.1.3",
    "history": "^4.7.2",
    "html-react-parser": "^0.6.2",
    "immutability-helper": "^3.0.0",
    "immutable": "^3.8.2",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.11",
    "lodash.reduce": "^4.6.0",
    "lodash.times": "^4.3.2",
    "moment": "^2.23.0",
    "now": "^13.0.6",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-favicon": "0.0.14",
    "react-flexbox-grid": "^2.1.2",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.4",
    "react-loadable": "^5.3.1",
    "react-player": "^1.8.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.1",
    "react-router-redux": "^4.0.8",
    "react-share": "^2.4.0",
    "reading-time": "^1.1.3",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reselect": "^4.0.0",
    "webfontloader": "^1.6.28"
  },
  "jest": {
    "testURL": "http://www.SomeAwesome.com",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/",
      "<rootDir>/node_modules/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setup/jestTestSetup.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "automock": false,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|json)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "webpack-dev-server",
    "postcss",
    "jest",
    "boilerplate",
    "hmr"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/utils/epubjs/**/*.js"
    ]
  }
}
